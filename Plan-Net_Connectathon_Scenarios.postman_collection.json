		{
			"name": "Scenario 5a: Request Organization data for a specific Provider",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "b0f639ca-80a4-489a-b085-d952c32c54c1",
						"exec": [
							"tests[\"Status code is 200\"] = responseCode.code === 200;",
							"",
							"// print out the step count to the console",
							"if (responseCode.code === 200){",
							"    ",
							"        try{",
							"            var dr_data = JSON.parse(responseBody);",
							"            var data_entry = dr_data.entry;",
							"            var data_entry_len = data_entry.length;",
							"            var orgs = [];",
							"            var org_id = [];",
							"            var hcs = [];",
							"            var oa = [];",
							"            var pr = [];",
							"            var org_id = [];",
							"            for (i = 0; i < data_entry_len; i++){",
							"                tests[\"Resource[\" + i + \"]:\" + data_entry[i].resource.resourceType] = true;",
							"                if (data_entry[i].resource.resourceType == \"Practitioner\"){",
							"                    var p_name  = data_entry[i].resource.name[0].given.join(' ') + \" \" + data_entry[i].resource.name[0].family;",
							"                    tests[\"Practitioner:\" + p_name] = true;",
							"                }",
							"",
							"                else if (data_entry[i].resource.resourceType == \"PractitionerRole\"){",
							"                   tests[\"PractitionerRole:\" + data_entry[i].resource.practitioner.display] = true;",
							"                   //tests[\"==organization==> \" + data_entry[i].resource.organization.reference] = true;",
							"                   pr.push(data_entry[i].resource.practitioner.display ); ",
							"",
							"                   //org_id.push(data_entry[i].resource.organization.reference.slice(13,));",
							"                   //        org_id.push(data_entry[i].resource.participatingOrganization.reference.slice(13,));// slice off ",
							"                }",
							"",
							"                else if (data_entry[i].resource.resourceType == \"Organization\"){",
							"                    tests[\"Organization:\" + data_entry[i].resource.name] = true;",
							"                    //orgs.push (\"Organization: \" + data_entry[i].resource.name )",
							"                }",
							"                else if (data_entry[i].resource.resourceType == \"Location\"){",
							"                    var l_name  = \"Location: \" + data_entry[i].resource.description; ",
							"                    tests[\"Location:\" + l_name] = true;",
							"                }",
							"                else if (data_entry[i].resource.resourceType == \"HealthcareService\"){",
							"                    hcs.push(\"HealthcareService: \" + data_entry[i].resource.name);",
							"                    tests[\"HealthcareService: \" + data_entry[i].resource.name] = true;",
							"                }",
							"                else if (data_entry[i].resource.resourceType == \"OrganizationAffiliation\"){",
							"                    oa.push(data_entry[i].resource.organization.display + \": \" + data_entry[i].resource.participatingOrganization.display);",
							"                    org_id.push(data_entry[i].resource.id);",
							"                    tests[ org_id.length + \" OrganizationAffiliation ids:\" + org_id.join(\", \")] = true;",
							"                    tests[\"OrganizationAffiliation: \" + data_entry[i].resource.organization.display + \": \" + data_entry[i].resource.participatingOrganization.display ] = true;",
							"                    ",
							"                }",
							"            }",
							"               ",
							"            tests[\"PractitionerRole:\" + pr.join(\", \")] = true;",
							"            tests[\"Organization:\" + orgs.join(\", \")] = true;",
							"            tests[\"HealthcareServices:\" + hcs.join(\", \")] = true;",
							"            tests[ dr_data.total + \" OrganizationAffiliations:\" + oa.join(\", \")] = true;",
							"            tests[ org_id.length + \" OrganizationAffiliation ids:\" + org_id.join(\", \")] = true;",
							"",
							"            pm.environment.set(\"pract_assoc_orgs\", org_id.join(\", \"));      ",
							"",
							"        }",
							"        catch(e){",
							"            console.log(e);",
							"            }",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
